	partition 'dCalendário Entregas' = calculated
		mode: import
		source =
				
				ADDCOLUMNS(
				    CALENDAR(
				        MIN('fColetas'[PREVISAO_DE_ENTREGA]),
				        EOMONTH(MAX('fColetas'[PREVISAO_DE_ENTREGA]), 0)),
				        "ANO",YEAR([Date]),
				        "MÊS", MONTH([Date]),
				        "DIA", DAY([Date]),
				        "ANO-MÊS", FORMAT([Date],"YYYY-MM"),
				        "ANO-NOME MÊS", UPPER(FORMAT([Date],"MMM-YY")),
				        "NOME DIA DA SEMANA", UPPER(FORMAT([Date],"DDDD")),
				        "DIA DA SEMANA", WEEKDAY([Date]),
				        "NOME DO MÊS", FORMAT([Date], "MMMM"),
				        "TRIMESTRE", FORMAT([Date],"Q") & " TRIMESTRE",
				        "CICLO", SWITCH(TRUE(),
				            DAY([Date]) <= 7, "1 SEMANA",
				            DAY([Date]) <= 14, "2 SEMANA",
				            DAY([Date]) <= 21, "3 SEMANA",
				            DAY([Date]) >= 22, "4 SEMANA")
				)

	annotation PBI_Id = 3a22f2ad30d540328a3d26342550b777

