	partition 'dCalendário Coletas' = calculated
		mode: import
		source =
				
				ADDCOLUMNS(
				    CALENDAR(
				        MIN('fColetas'[PREVISAO_DE_COLETA]),
				        EOMONTH(MAX('fColetas'[PREVISAO_DE_COLETA]), 0)),
				        "ANO",YEAR([Date]),
				        "MÊS", MONTH([Date]),
				        "DIA", DAY([Date]),
				        "ANO-MÊS", FORMAT([Date],"YYYY-MM"),
				        "ANO-NOME MÊS", UPPER(FORMAT([Date],"MMM-YY")),
				        "NOME DIA DA SEMANA", UPPER(FORMAT([Date],"DDDD")),
				        "DIA DA SEMANA", WEEKDAY([Date]),
				        "NOME DO MÊS", FORMAT([Date], "MMMM"),
				        "TRIMESTRE", FORMAT([Date],"Q") & " TRIMESTRE",
				        "CICLO", SWITCH(TRUE(),
				            DAY([Date]) <= 7, "1 SEMANA",
				            DAY([Date]) <= 14, "2 SEMANA",
				            DAY([Date]) <= 21, "3 SEMANA",
				            DAY([Date]) >= 22, "4 SEMANA")
				)

	annotation PBI_Id = 6801e03bb120438cbe339153c7e96493

