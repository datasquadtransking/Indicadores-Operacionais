	measure %NOPRAZO_COLETA_D0 = ```
			
			VAR TotalItens = CALCULATE(
			    DISTINCTCOUNT(fColetas[COTACAO]),
			    MONTH('dCalendário Coletas'[Date]) = MONTH(TODAY()),
			    YEAR('dCalendário Coletas'[Date]) = YEAR(TODAY())
			)
			VAR TotalOTC = CALCULATE(
			    DISTINCTCOUNT(fColetas[COTACAO]),
			    'fColetas'[PERFORMANCE_DE_COLETA] = "Coleta no Prazo",
			    MONTH('dCalendário Coletas'[Date]) = MONTH(TODAY()),
			    YEAR('dCalendário Coletas'[Date]) = YEAR(TODAY())
			)
			VAR TotalOTC2 = CALCULATE(
			    DISTINCTCOUNT(fColetas[COTACAO]),
			    'fColetas'[PERFORMANCE_DE_COLETA] = "Dentro do Prazo para Coletar",
			    MONTH('dCalendário Coletas'[Date]) = MONTH(TODAY()),
			    YEAR('dCalendário Coletas'[Date]) = YEAR(TODAY())
			)
			VAR SumOTC = TotalOTC + TotalOTC2
			VAR PERCENTOTC = DIVIDE(SumOTC, TotalItens)
			VAR TotalColetas = TotalItens  -- Já conta o número total de coletas do mês atual
			RETURN
			IF(
			    TotalColetas = 0,   -- Verifica se não existem valores na coluna 'fColetas[NÚMERO COLETA]' no mês atual
			    BLANK(),            -- Retorna BLANK() se não houver valores
			    IF(
			        ISBLANK(PERCENTOTC),  -- Verifica se PERCENTOTD é nulo
			        0,                     -- Retorna zero se for nulo
			        PERCENTOTC             -- Caso contrário, retorna o valor calculado de PERCENTOTD
			    )
			)
			
			```
		formatString: 0.0%;-0.0%;0.0%
		displayFolder: 0. KPIs
		lineageTag: abd0babc-104d-4e5c-84a2-66819d712bec

