	measure %NOPRAZO_COLETA = ```
			
			VAR TotalItens = DISTINCTCOUNT('fColetas'[COTACAO])
			VAR TotalOTC = CALCULATE(DISTINCTCOUNT('fColetas'[COTACAO]), 'fColetas'[PERFORMANCE_DE_COLETA] = "Coleta no Prazo")
			VAR TotalOTC2 = CALCULATE(DISTINCTCOUNT('fColetas'[COTACAO]), 'fColetas'[PERFORMANCE_DE_COLETA] = "Dentro do Prazo para Coletar")
			VAR SumOTC = TotalOTC + TotalOTC2
			VAR PERCENTOTC = DIVIDE(SumOTC, TotalItens)
			VAR TotalColetas = DISTINCTCOUNT('fColetas'[COTACAO]) -- Conta o número total de coletas (COTACAO)
			RETURN
			IF(
			    TotalColetas = 0,   -- Verifica se não existem valores na coluna 'fColetas[COTACAO]'
			    BLANK(),            -- Retorna BLANK() se não houver valores
			    IF(
			        ISBLANK(PERCENTOTC),  -- Verifica se PERCENTOTC é nulo
			        0,                     -- Retorna zero se for nulo
			        PERCENTOTC             -- Caso contrário, retorna o valor calculado de PERCENTOTC
			    )
			)
			
			```
