	measure %NOPRAZO_ENTREGA = ```
			
			VAR TotalItens = COUNTROWS('fColetas')
			VAR TotalOTD = CALCULATE(COUNTROWS('fColetas'), 'fColetas'[PERFORMANCE_DE_ENTREGA] = "ENTREGA NO PRAZO")
			VAR TotalOTD2 = CALCULATE(COUNTROWS('fColetas'), 'fColetas'[PERFORMANCE_DE_ENTREGA] = "DENTRO DO PRAZO PARA ENTREGAR")
			VAR SumOTD = TotalOTD + TotalOTD2
			VAR PERCENTOTD = DIVIDE(SumOTD, TotalItens)
			VAR TotalColetas = COUNTROWS('fColetas') -- Conta o número total de coletas
			RETURN
			IF(
			    TotalColetas = 0,   -- Verifica se não existem valores na coluna 'fColetas[NÚMERO COLETA]'
			    BLANK(),            -- Retorna BLANK() se não houver valores
			    IF(
			        ISBLANK(PERCENTOTD),  -- Verifica se PERCENTOTD é nulo
			        0,                     -- Retorna zero se for nulo
			        PERCENTOTD             -- Caso contrário, retorna o valor calculado de PERCENTOTD
			    )
			)
			
			```
		formatString: 0.0%;-0.0%;0.0%
		displayFolder: 0. KPIs
		lineageTag: 5ae087e9-56a5-4711-9fb7-3ec39ab210f3

